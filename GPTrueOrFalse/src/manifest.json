{
  "manifest_version": 2,
  "name": "GPTrue or False",
  "version": "1.0.0",
  "description": "Display OpenAI's GPT-2 log probability of a sample of text.",
  "icons": {
    "16": "./images/icon/16.png",
    "32": "./images/icon/32.png",
    "64": "./images/icon/64.png",
    "128": "./images/icon/128.png"
  },
  "author": "Giulio Starace",
  "permissions": [
    "https://huggingface.co/*"
  ],
  "content_scripts": [
    {
      "css": [
        "content/content.css"
      ],
      "js": [
        "libraries/browser-polyfill.min.js",
        "libraries/jquery-3.3.1.slim.min.js",
        "content/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "./images/icon/16.png",
      "32": "./images/icon/32.png",
      "64": "./images/icon/64.png",
      "128": "./images/icon/128.png"
    },
    "default_popup": "popup/index.html",
    "default_title": "GPTrue or False"
  }
}